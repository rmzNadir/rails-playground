<%= form_with model: @photo do |f| %>
   <div>
        <%= f.label :title%>
        <%= f.text_field :title%>
    </div>

    <div>
        <%= f.label :image_url%>
        <%= f.url_field :image_url%>

    </div>

    <%= f.submit "Guardar"%>
<%end%>

<div>
    <%= link_to 'Cancelar', photo_path(@photo.id) %>
</div>